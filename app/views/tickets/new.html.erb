<h1>Create a Ticket</h1>
<%= form_for @ticket do |f| %>
    <p>
        <%= f.label :title %><br/>
        <%= f.text_field :title %>
    </p>
    <p>
        <%= f.label :description %><br/>
        <%= f.text_field :description %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>

